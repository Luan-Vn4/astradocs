{
	"name": "@astradocs/cli",
	"description": "An AI powered tool which aims to provide automatic documentation augmentad by context information",
	"version": "0.0.0",
	"author": "Luan-Vn4",
	"bin": {
		"astra": "./bin/run.js"
	},
	"bugs": "https://github.com/Luan-Vn4/astradocs/issues",
	"dependencies": {
		"@oclif/core": "^4.8.0",
		"@oclif/plugin-help": "^6.2.37",
		"@oclif/plugin-plugins": "^5.4.55",
		"@astradocs/core": "workspace:*"
	},
	"devDependencies": {
		"@oclif/prettier-config": "^0.2.1",
		"@oclif/test": "^4.1.16",
		"@types/chai": "^5.2.3",
		"@types/mocha": "^10.0.10",
		"@types/node": "^25.2.0",
		"chai": "^6.2.2",
		"eslint": "^9.39.2",
		"eslint-config-oclif": "^6.0.137",
		"eslint-config-prettier": "^10.1.8",
		"mocha": "^11.7.5",
		"oclif": "^4.22.73",
		"shx": "^0.4.0",
		"ts-node": "^10.9.2",
		"typescript": "^5.9.3"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"files": [
		"./bin",
		"./dist",
		"./oclif.manifest.json"
	],
	"homepage": "https://github.com/Luan-Vn4/astradocs",
	"keywords": [
		"oclif"
	],
	"license": "MIT",
	"main": "dist/index.js",
	"type": "module",
	"oclif": {
		"bin": "astra",
		"dirname": "astra",
		"commands": "./dist/commands",
		"plugins": [
			"@oclif/plugin-help",
			"@oclif/plugin-plugins"
		],
		"topicSeparator": " ",
		"topics": {
			"hello": {
				"description": "Say hello to the world and others"
			}
		}
	},
	"repository": "Luan-Vn4/astradocs",
	"scripts": {
		"start": "bun bin/run.js",
		"dev": "bun bin/dev.js",
		"build": "bun run clean && tsc -b",
		"clean": "shx rm -rf dist tsconfig.tsbuildinfo",
		"postpack": "shx rm -f oclif.manifest.json",
		"posttest": "bun run lint && bun run typecheck",
		"prepack": "oclif manifest && oclif readme",
		"test": "bun test",
		"version": "oclif readme && git add README.md"
	},
	"types": "dist/index.d.ts"
}
